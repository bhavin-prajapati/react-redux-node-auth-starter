{
  "name": "react-redux-node-auth-starter",
  "version": "1.0.0",
  "description": "server using resource owner password credentials grant",
  "main": "server.js",
  "scripts": {
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:setup": "npm run db:create; npm run db:migrate; npm run db:seed;",
    "start": "nodemon server.js --exec babel-node --presets es2015,stage-2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bhavin-prajapati/react-redux-node-auth-starter.git"
  },
  "author": "Bhavin Prajapati",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bhavin-prajapati/react-redux-node-auth-starter/issues"
  },
  "homepage": "https://github.com/bhavin-prajapati/react-redux-node-auth-starter",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.3.0",
    "mysql2": "^1.5.2",
    "sequelize": "^4.35.5",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.11.6",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.0"
  }
}
